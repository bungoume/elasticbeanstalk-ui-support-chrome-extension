{
  "manifest_version": 3,
  "name": "ElasticBeanstalk UI Support",
  "version": "1.2.0",
  "description": "AWS ElasticBeanstalk UI Support Extension",
  "content_scripts": [{
    "matches": [
      "https://*.console.aws.amazon.com/elasticbeanstalk/*"
    ],
    "js": [
      "content.js"
    ]
  }],
  "host_permissions": [
    "https://*.console.aws.amazon.com/elasticbeanstalk/*"
  ],
  "icons": {"128": "icon128.png"}
}
